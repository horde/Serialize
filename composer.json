{
    "minimum-stability": "dev",
    "name": "horde/serialize",
    "description": "Data serialization library",
    "type": "library",
    "homepage": "https://www.horde.org/libraries/Horde_Serialize",
    "license": "LGPL-2.1-only",
    "authors": [
        {
            "name": "Jan Schneider",
            "email": "jan@horde.org",
            "role": "lead"
        },
        {
            "name": "Chuck Hagenbuch",
            "email": "chuck@horde.org",
            "role": "lead"
        },
        {
            "name": "Michael Slusarz",
            "email": "slusarz@horde.org",
            "role": "developer"
        }
    ],
    "time": "2022-11-04",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8",
        "horde/exception": "^3 || dev-FRAMEWORK_6_0",
        "horde/util": "^3 || dev-FRAMEWORK_6_0"
    },
    "require-dev": {},
    "suggest": {
        "horde/imap_client": "^3 || dev-FRAMEWORK_6_0",
        "horde/mime": "^3 || dev-FRAMEWORK_6_0",
        "horde/test": "^3 || dev-FRAMEWORK_6_0",
        "ext-bz2": "*",
        "ext-json": "*",
        "ext-wddx": "*",
        "ext-zlib": "*",
        "ext-lzf": "*"
    },
    "autoload": {
        "psr-0": {
            "Horde_Serialize": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Horde\\Serialize\\Test\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "horde/horde-installer-plugin": true
        }
    }
}